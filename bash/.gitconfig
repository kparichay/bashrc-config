[user]
  name = Parichay Kapoor
  email = pk.kapoor@samsung.com
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[credential]
  helper = store
[core]
  excludesfile = ~/.gitignore
  checkStat = minimal
  trustctime = false
  untrackedCache = true
  editor = vim
[diff]
  guitool = default-difftool
  tool = default-difftool
  renames = copies
[difftool "default-difftool"]
  cmd = code --wait --diff $LOCAL $REMOTE
[difftool]
  prompt = false
[push]
  default = simple
[pull]
  rebase = false
